
<div class="row project-list justify-content-center">
  <div class="container">
  <%# title %>
    <div class="projects-page-title col-12 col-lg-6">
      <div class="title">
          <h1>My Learning Tracks</h1>
      </div>
  <%# paw %>
    <div class="paw">
        <ul>
          <li><i class="fa-solid fa-paw yellow-paw"></i></li>
          <li><i class="fa-solid fa-paw yellow-paw-center"></i></li>
          <li><i class="fa-solid fa-paw yellow-paw"></i></li>
        </ul>
      </div>
    </div>
  <%# project card %>
    <div class="showpage-projects-card">
      <div class="content">
        <div class="content-top">
          <div class="category-tag">
            <p><%= @project.category %></p>
          </div>
            <button class="green-btn">
              <%= link_to "Add", projects_path%>
            </button>
          </div>
            <h6><strong><%= @project.name %></strong></h6>
            <p><%= @project.description%></p>
          <div class="content-bottom">
            <p>Deadline:<br><small><%= @project.deadline %></small></p>
          </div>
      </div>
    </div>

    <div class="showpage-projects-card">
      <div class="content">
        <div class="status">
          <h6><strong>Status:</strong></h6>
          <p>My Progress:</p>
          <p>Key Concepts Learned:</p>
          <p>Points earned:</p>
        </div>
      </div>
    </div>
  </div>
</div>
